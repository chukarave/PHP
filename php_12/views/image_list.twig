{% extends "layout.twig" %}

{% block content %}
<u1>
    {% for image in images %}
        <li>
            <a href="/{{image.id}}" title="{{image.title}}">
            <h2>{{image.title}}</h2>
            <img src="{{image.url}}" alt="{{image.title}}" />
            </a>
        </li>
    {% endfor %}
</u1>
{% endblock %}
